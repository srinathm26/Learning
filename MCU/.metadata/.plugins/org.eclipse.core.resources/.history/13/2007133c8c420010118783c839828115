#include <stdint.h>
#include "stm32f407xx.h"
#include "stm32f407xx_gpio_driver.h"

int main(void)
{
	GPIO_Handle_t GpioLED;
	GpioLED.pGPIOx = GPIOD;
	GpioLED.PinConfig.PinNumber = GPIO_PIN_12;
	GpioLED.PinConfig.PinMode = GPIO_MODE_OP;
	GpioLED.PinConfig.PinOPtype = GPIO_OP_TYPE_PP;
	GpioLED.PinConfig.PinSpeed = GPIO_OP_SPEED_HI;
	GpioLED.PinConfig.PinPuPdControl = GPIO_PULL_NO;

	GPIO_Init(GpioLED);
	GPIO_ToggleOPPin(GpioLED.pGPIOx ,GpioLED.PinConfig.PinNumber);
	for(;;);
}
